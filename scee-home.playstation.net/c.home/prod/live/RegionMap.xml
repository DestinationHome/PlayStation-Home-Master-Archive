        <MAP code="no-*" >EU-Scandinavian</MAP>
        <MAP code="da-*" >EU-Scandinavian</MAP>
        <MAP code="fi-*" >EU-Scandinavian</MAP>
        <MAP code="nl-*" >EU-Benelux</MAP>
        <MAP code="*-BE" >EU-Benelux</MAP>
        <MAP code="*-LU" >EU-Benelux</MAP>
        <MAP code="*-RU" >EU-Russian</MAP>
        <MAP code="*-AE" >EU-English</MAP>
        <MAP code="*-ZA" >EU-English</MAP>-->
        <MAP code="ja-*" >Asia-Japan</MAP>
        <MAP code="ko-*" >Asia-Korea</MAP>
        <MAP code="*-HK" >Asia-HongKong</MAP>
        <MAP code="*-TW" >Asia-Taiwan</MAP>
