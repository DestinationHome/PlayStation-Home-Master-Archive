<XML>
    <REGION_TYPES>
        <!-- region name, territory and region tag (also default localisation) -->
        <TYPE name="EU-GB" territory="SCEE">en-GB</TYPE>
        <TYPE name="EU-France" territory="SCEE">fr-FR</TYPE>
        <TYPE name="EU-Italy" territory="SCEE">it-IT</TYPE>
        <TYPE name="EU-Germany" territory="SCEE">de-DE</TYPE>
        <TYPE name="EU-Spain" territory="SCEE">es-ES</TYPE>
