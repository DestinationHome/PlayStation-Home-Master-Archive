<?xml version="1.0"?>
<cocktail_menu>
<do_the_flash>1</do_the_flash>
</cocktail_menu>

<!--
	Babu Madhikarmi
	nDreams Ltd
	
	This will switch the Cocktail Menus flash icons on or off, in the Playstation Plus. Use values 1 or 0.
	### To make it flash, set above to the following:
	<cocktail_menu>
	<do_the_flash>1</do_the_flash>
	</cocktail_menu>
	
	### To stop it flashing, set above to the following:
	<cocktail_menu>
	<do_the_flash>0</do_the_flash>
	</cocktail_menu>
-->