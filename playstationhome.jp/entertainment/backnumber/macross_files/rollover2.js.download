	roImg: function(){
		if(this.tagName){
			var imgTagData = this.getTag('img');
			var useImg = [];
			var useCount = 0;

			for(i=0; i<imgTagData.length; i++){
				var imgSrc = imgTagData[i].getAttribute("src");
				if(imgSrc.match(FKro.regOver)){
					this.preload(imgSrc,FKro.regOver,overImgName);

					if(rollOverMap){
