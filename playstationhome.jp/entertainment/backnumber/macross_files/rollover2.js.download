var FKroFormat = function(over,out){
	this.tagName = FK.doc.getElementsByTagName;
	this.regOver = new RegExp(outImgName + "+(\.[a-z]+)$");
	this.regOut  = new RegExp(overImgName + "+(\.[a-z]+)$");
};

FKroFormat.prototype = {
	init: function(){
		if(rollOverImg) FKro.roImg();
